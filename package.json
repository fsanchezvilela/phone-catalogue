{
  "name": "phone-catalogue",
  "version": "1.0.0",
  "description": "Full stack application to manage a phone catalogue",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "docker:build": "docker build -t phone-catalogue .",
    "docker:run": "docker run -p 3000:3000 phone-catalogue",
    "docker:run:dev": "docker run -p 3000:3000 -v $(pwd):/app phone-catalogue",
    "docker:compose": "docker compose up --build",
    "docker:compose:dev": "docker compose -f docker-compose.dev.yml up --build",
    "test": "echo \"No tests specified\" && exit 0",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "lint-staged": "lint-staged",
    "format": "prettier --write .",
    "prettier": "prettier --check .",
    "clean": "rm -rf node_modules dist .tanstack",
    "clean:docker": "docker system prune -f && docker volume prune -f",
    "start": "vite preview",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "Fernando Sanchez",
  "license": "MIT",
  "dependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/react-query": "^5.66.0",
    "@tanstack/react-query-devtools": "^5.66.0",
    "@tanstack/react-router": "^1.130.12",
    "@tanstack/react-router-devtools": "^1.130.13",
    "@tanstack/react-router-with-query": "^1.130.12",
    "@tanstack/react-start": "^1.130.15",
    "globals": "^16.3.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwindcss": "^4.1.11",
    "vite": "^7.0.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/node": "^22.5.4",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-react": "^4.7.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
